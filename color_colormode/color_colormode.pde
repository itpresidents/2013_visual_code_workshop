void setup() {
  size(600, 400);
  // colorMode allows you to change the mode and the max values.
  // There are two main color modes RGB and HSB
  // RGB is Red, Green and Blue and HSB is Hue, Saturation and Brightness
  colorMode(RGB, 255);
  // colorMode(RGB, 100, 100, 100, 100);
  // colorMode(HSB, 360, 100, 100);
  background(255, 0, 0, 100);
  noStroke();
}
